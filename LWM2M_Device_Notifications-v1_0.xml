<?xml version="1.0" encoding="UTF-8"?>
<LWM2M  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://openmobilealliance.org/tech/profiles/LWM2M.xsd">
  <Object ObjectType="MODefinition">
    <Name>Device Notifications</Name>
    <Description1>Indicates when various event notifications were received from the cube.</Description1>
    <ObjectID>34950</ObjectID>
    <ObjectURN>urn:oma:lwm2m:x:34950</ObjectURN>
    <LWM2MVersion>1.0</LWM2MVersion>
    <ObjectVersion>1.2</ObjectVersion>
    <MultipleInstances>Multiple</MultipleInstances>
    <Mandatory>Optional</Mandatory>
    <Resources>
      <Item ID="0">
          <Name>Factory Reset Indicator</Name>
          <Operations>RW</Operations>
          <MultipleInstances>Single</MultipleInstances>
          <Mandatory>Mandatory</Mandatory>
          <Type>Integer</Type>
          <RangeEnumeration></RangeEnumeration>
          <Units></Units>
          <Description>Indicates if a factory reset occurred on the cube.</Description>
      </Item>
      <Item ID="1">
          <Name>Reconnect Indicator</Name>
          <Operations>RW</Operations>
          <MultipleInstances>Single</MultipleInstances>
          <Mandatory>Mandatory</Mandatory>
          <Type>Integer</Type>
          <RangeEnumeration></RangeEnumeration>
          <Units></Units>
          <Description>Indicates if a reconnection event occurred on the cube.</Description>
      </Item>
      <Item ID="2">
          <Name>Filter Reset Indicator</Name>
          <Operations>RW</Operations>
          <MultipleInstances>Single</MultipleInstances>
          <Mandatory>Mandatory</Mandatory>
          <Type>Boolean</Type>
          <RangeEnumeration></RangeEnumeration>
          <Units></Units>
          <Description>Confirms that the filter was changed, set to true by purifier when filter is reset</Description>
      </Item>
    </Resources>
    <Description2></Description2>
    </Object>
</LWM2M>